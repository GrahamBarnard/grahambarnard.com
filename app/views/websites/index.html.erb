<h1>Websites</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th>Description</th>
    <th></th>
  </tr>

<% @websites.each do |website| %>
  <tr>
    <td><%= website.name %></td>
    <td><%= website.url %></td>
    <td><%= website.description %></td>
  </tr>
<% end %>
</table>
<%= link_to 'New Site Entry', new_website_path %>

